<div class="container">
    <div class="row">
        @model AdminRegisterViewModel
@{
    ViewData["Title"] = "Create";
}

<form asp-action="create" method="post">
    <div class="form-group">
        <label asp-for="FullName">FullName</label>
        <input type="text" class="form-control" asp-for="FullName" placeholder="Enter FullName">
        <span asp-validation-for="FullName"></span>
    </div>
    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input type="text" class="form-control" asp-for="Email" placeholder="Enter Email">
        <span asp-validation-for="Email"></span>
    </div>
    <div class="form-group">
        <label asp-for="Username">Username</label>
        <input type="text" class="form-control" asp-for="Username" placeholder="Enter Username">
        <span asp-validation-for="Username"></span>
    </div>
    <div class="form-group">
        <label asp-for="Password">Password</label>
        <input type="text" class="form-control" asp-for="Password" placeholder="Enter Password">
        <span asp-validation-for="Password"></span>
    </div>
    <div asp-validation-summary="ModelOnly"></div>
    <button type="submit" class="btn btn-primary mb-2">Create</button>
</form>


    </div>
</div>